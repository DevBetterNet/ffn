	<link href="@Url.Content($"~/shop/css/preloader.css")" type="text/css" rel="stylesheet" media="screen" />

	<link href="@Url.Content($"~/shop/libs/materialize/materialize.min.css")" type="text/css" rel="stylesheet" media="screen" />
	<link href="@Url.Content($"~/shop/libs/fonts/mdi/appicon/appicon.css")" type="text/css" rel="stylesheet" media="screen" />
	<link href="@Url.Content($"~/shop/libs/fonts/mdi/materialdesignicons.min.css")" type="text/css" rel="stylesheet" media="screen" />
	<link href="@Url.Content($"~/shop/libs/perfect-scrollbar/perfect-scrollbar.css")" type="text/css" rel="stylesheet" media="screen" />
	<link href="@Url.Content($"~/shop/libs/fancybox/jquery.fancybox.min.css")" rel="stylesheet" type="text/css" media="screen" />
	<link href="@Url.Content($"~/shop/css/style.css")" type="text/css" rel="stylesheet" media="screen" id="main-style" />
</head>
<body class="html" data-header="light" data-footer="dark" data-header_align="center" data-menu_type="left" data-menu="light" data-menu_icons="on" data-footer_type="left" data-site_mode="light" data-footer_menu="show" data-footer_menu_style="light">
	<div class="preloader-background">
		<div class="preloader-wrapper">
			<div class="orig">

				<div class="dot orig three "></div>
				<div class="dot orig two "></div>
				<div class="dot orig one "></div>
			</div>

		</div>
	</div>

	@await Html.PartialAsync("_Navigation.cshtml")

	<div class="content-area">
		@RenderBody()

		@await Html.PartialAsync("_Footer.cshtml")
	</div>

	@*<script src="@Url.Content($"~/shop/js/pwa.js")"></script>*@
	<script src="@Url.Content($"~/shop/libs/jquery/jquery-2.2.4.min.js")"></script>
	<script src="@Url.Content($"~/shop/libs/materialize/materialize.js")"></script>
	<script src="@Url.Content($"~/shop/libs/perfect-scrollbar/perfect-scrollbar.min.js")"></script>
	<script src="@Url.Content($"~/shop/js/variables.js")"></script>
	<script type="text/javascript">
		$(document).ready(function () {

		  $(".carousel-fullscreen.carousel-slider").carousel({
			fullWidth: true,
			indicators: true
		  });
		  setTimeout(autoplay, 3500);
		  function autoplay() {
			$(".carousel").carousel("next");
			setTimeout(autoplay, 3500);
		  }
		  $(".slider8").slider({
			indicators: false,
			height: 210,
		  });

		});
	</script>
	@*<script src="assets/js/common.html"></script>*@
	<script src="@Url.Content($"~/shop/libs/app/init.js")"></script>
	<script src="@Url.Content($"~/shop/libs/app/settings.js")"></script>
	<script src="@Url.Content($"~/shop/libs/app/scripts.js")"></script>
	<script src="@Url.Content($"~/shop/js/preloader.js")"></script>

	<script async src="https://www.googletagmanager.com/gtag/js?id=UA-168736414-1"></script>
	<script>
		window.dataLayer = window.dataLayer || [];
		function gtag(){dataLayer.push(arguments);}
		gtag('js', new Date());

		gtag('config', 'UA-168736414-1');
	</script>
</body>

	<link rel="icon" type="image/png" sizes="512x512" href="@Url.Content($"~/shop/images/icons/android-icon-512x512.png")">
	<link rel="icon" type="image/png" sizes="32x32" href="@Url.Content($"~/shop/images/icons/favicon-32x32.png")">
	<link rel="icon" type="image/png" sizes="96x96" href="@Url.Content($"~/shop/images/icons/favicon-96x96.png")">

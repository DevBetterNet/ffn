	<title>Tiệm Cô Tít - Trang chủ</title>

	<!-- App Icons -->
	<link rel="apple-touch-icon" sizes="57x57" href="@Url.Content($"~/shop/images/icons/apple-icon-57x57.png")">
	<link rel="apple-touch-icon" sizes="60x60" href="@Url.Content($"~/shop/images/icons/apple-icon-60x60.png")">
	<link rel="apple-touch-icon" sizes="72x72" href="@Url.Content($"~/shop/images/icons/apple-icon-72x72.png")">
	<link rel="apple-touch-icon" sizes="76x76" href="@Url.Content($"~/shop/images/icons/apple-icon-76x76.png")">
	<link rel="apple-touch-icon" sizes="114x114" href="@Url.Content($"~/shop/images/icons/apple-icon-114x114.png")">
